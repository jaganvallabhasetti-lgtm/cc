#User function Template for python3

class Solution:
    def segregateElements(self, arr):
        # Your code goes here
        result=[]
        result1=[]
        result2=[]
        for ele in arr:
            if ele>=0:
                result1.append(ele)
            else:
                result2.append(ele)
        result=result1+result2
        for i in range(len(arr)):
            arr[i]=result[i]
        return arr
